<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ChessEnginePath" value=".\stockfish-5-win\stockfish_14053109_x64.exe"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/></startup>
  <system.diagnostics>
    <sources>
      <source name="ChessCommandTraceSource"
        switchName="chessCommandSwitch"
        switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="textListener"
            type="System.Diagnostics.TextWriterTraceListener"
            initializeData="chessCommandTraceSource.txt"
            traceOutputOptions="ProcessId, DateTime" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="UniversalChessInterfaceTraceSource"
       switchName="universalChessInterfaceSwitch"
       switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="textListener"
            type="System.Diagnostics.TextWriterTraceListener"
            initializeData="universalChessInterfaceTraceSource.txt"
            traceOutputOptions="ProcessId, DateTime" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="EngineControllerTraceSource"
        switchName="engineControllerSwitch"
        switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="textListener"
            type="System.Diagnostics.TextWriterTraceListener"
            initializeData="engineControllerTraceSource.txt"
            traceOutputOptions="ProcessId, DateTime" />
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="chessCommandSwitch" value="Off"/>
      <add name="universalChessInterfaceSwitch" value="Off"/>
      <add name="engineControllerSwitch" value="Off"/>
    </switches>
    <trace autoflush="true" indentsize="4" />
  </system.diagnostics>
</configuration>
